{
  "name": "Sales Assist",
  "version": "0.1.0",
  "description": "Assist for Sales in meetings",
  "icons": {
    "16": "images/sales_assist_16.png",
    "32": "images/sales_assist_32.png",
    "48": "images/sales_assist_48.png",
    "128": "images/sales_assist_128.png"
  },
  "web_accessible_resources": [
    "images/sales_assist_32.png"
  ],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "exclude_matches": ["https://meet.google.com/", "https://meet.google.com/?*"],
      "js": ["assist.js"],
      "run_at": "document_idle"
    }
  ],
  "key": ""
}
